{
    "name": "dlds/yii2-authme",
    "description": "Yii2 Authorization component",
    "keywords": ["yii2", "authorization", "component"],
    "type": "yii2-extension",
    "license": "BSD",
    "support": {
        "issues": "https://github.com/dlds/yii2-authme/issues",
        "wiki": "https://github.com/dlds/yii2-authme/wiki",
        "source": "https://github.com/dlds/yii2-authme"
    },
    "authors": [
        {
            "name": "Jiri Svoboda",
            "email": "jiri.svoboda@dlds.cz"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-faker": "*",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "autoload": {
        "psr-4": {
            "dlds\\authme\\": "src"
        }
    }
}